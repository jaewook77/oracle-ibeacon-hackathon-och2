# This file is for unifying the coding style for different editors and IDEs
# editorconfig.org

# There must NOT be .editorconfig files above the folder which contains this file
root = true

[.git*]
charset = utf-8
end_of_line = lf
insert_final_newline = false
trim_trailing_whitespace = true

[**.{java,js}]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = false
trim_trailing_whitespace = true

[*.md]
charset = utf-8
end_of_line = lf
insert_final_newline = false
trim_trailing_whitespace = false